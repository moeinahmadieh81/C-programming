#include <stdio.h>
#include <string.h>

int main()
{
    char s[100];
    int i;
    gets(s);

    for(i = 0; s[i] != '\0'; i++)
    {
        if(s[0] >= 'a' && s[0] <= 'z')
            s[0] -= 32;

        if(s[i] >= 'A' && s[i] <= 'Z'){
            s[i] = s[i] + 32;
        }
    }
    printf("%s\n", s);
}
